issn = {1073-8584},
journal = {The Neuroscientist : a review journal bringing neurobiology, neurology and psychiatry},
keywords = {Animals,Behavior, Animal,Memory,Memory: physiology,Models, Neurological,Nerve Net,Nerve Net: physiology,Neuronal Plasticity,Neuronal Plasticity: physiology,Nitric Oxide,Nitric Oxide: physiology,Synaptic Transmission,Synaptic Transmission: physiology},
month = {apr},
