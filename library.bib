journal = {Neuron},
keywords = {Autistic Disorder,Autistic Disorder: physiopathology,Humans,Mental Processes,Mental Processes: physiology,Models, Biological,Photic Stimulation,Photic Stimulation: methods,Visual Fields,Visual Fields: physiology,Visual Perception,Visual Perception: physiology},
