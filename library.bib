month = {aug},
number = {8},
pages = {1402--14},
title = {{A neural model for compensation of sensory abnormalities in autism through feedback from a measure of global perception}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18701370},
volume = {19},
year = {2008}
}
