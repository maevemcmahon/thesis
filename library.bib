issn = {1460-2199},
journal = {Cerebral cortex (New York, N.Y. : 1991)},
keywords = {Animal,Animals,Disease Models,Down-Regulation,Fragile X Mental Retardation Protein,Fragile X Mental Retardation Protein: genetics,Fragile X Mental Retardation Protein: metabolism,Fragile X Syndrome,Fragile X Syndrome: metabolism,GABA-A,GABA-A: metabolism,Humans,Inbred C57BL,Knockout,Male,Mice,Neural Inhibition,Receptors,Synaptic Transmission,gamma-Aminobutyric Acid,gamma-Aminobutyric Acid: metabolism},
