isbn = {3315520593},
issn = {0010-0277},
keywords = {Adult,Age Factors,Attention,Auditory Perception,Child,Child Development,Child, Preschool,Female,Humans,Male,Models, Psychological,Music,Set (Psychology),Time Perception},
month = {dec},
number = {3},
pages = {251--88},
pmid = {11018511},
title = {{The development of rhythmic attending in auditory sequences: attunement, referent period, focal attending.}},
