@article{DiCastro2011,
abstract = {Astrocytes communicate with synapses by means of intracellular calcium ([Ca(2+)](i)) elevations, but local calcium dynamics in astrocytic processes have never been thoroughly investigated. By taking advantage of high-resolution two-photon microscopy, we identify the characteristics of local astrocyte calcium activity in the adult mouse hippocampus. Astrocytic processes showed intense activity, triggered by physiological transmission at neighboring synapses. They encoded synchronous synaptic events generated by sparse action potentials into robust regional (∼12 $\mu$m) [Ca(2+)](i) elevations. Unexpectedly, they also sensed spontaneous synaptic events, producing highly confined (∼4 $\mu$m), fast (millisecond-scale) miniature Ca(2+) responses. This Ca(2+) activity in astrocytic processes is generated through GTP- and inositol-1,4,5-trisphosphate-dependent signaling and is relevant for basal synaptic function. Thus, buffering astrocyte [Ca(2+)](i) or blocking a receptor mediating local astrocyte Ca(2+) signals decreased synaptic transmission reliability in minimal stimulation experiments. These data provide direct evidence that astrocytes are integrated in local synaptic functioning in adult brain.},
author = {{Di Castro}, Maria Amalia and Chuquet, Julien and Liaudet, Nicolas and Bhaukaurally, Khaleel and Santello, Mirko and Bouvier, David and Tiret, Pascale and Volterra, Andrea},
