keywords = {Acoustic Stimulation,Adult,Affect,Child Development,Child, Preschool,Dancing,Female,Humans,Infant,Infant Behavior,Male,Movement,Music,Music: psychology,Periodicity,Speech Acoustics},
month = {mar},
