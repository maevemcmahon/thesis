issn = {0962-8452},
journal = {Proceedings. Biological sciences / The Royal Society},
keywords = {Animal,Animal Communication,Animals,Choice Behavior,DNA-Binding Proteins,DNA-Binding Proteins: genetics,DNA-Binding Proteins: metabolism,Female,Gene Expression Regulation,Genes,Immediate-Early,Male,Neuronal Plasticity,Prosencephalon,Prosencephalon: metabolism,Sexual Behavior,Songbirds,Songbirds: genetics,Songbirds: physiology,Transcription Factors,Transcription Factors: genetics,Transcription Factors: metabolism,Vocalization,bird song,female choice,immediate early gene,neuroplasticity,sexual selection},
month = {dec},
number = {1508},
