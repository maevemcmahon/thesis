journal = {Neuroscience},
keywords = {Amnesia,Amnesia: chemically induced,Amnesia: physiopathology,Amnesia: prevention {\&} control,Amnesia: prevention {\{}{\&}{\}} control,Animals,Avoidance Learning,Avoidance Learning: drug effects,Avoidance Learning: physiology,Male,Neural Inhibition,Neural Inhibition: drug effects,Neural Inhibition: physiology,Nicotine,Nicotine: toxicity,Nicotinic Agonists,Nicotinic Agonists: toxicity,Nitric Oxide,Nitric Oxide: physiology,Rats,Tobacco Use Disorder,Tobacco Use Disorder: metabolism,Tobacco Use Disorder: physiopathology,Treatment Outcome,Ventral Tegmental Area,Ventral Tegmental Area: drug effects,Ventral Tegmental Area: metabolism,Ventral Tegmental Area: physiopathology,Wistar},
