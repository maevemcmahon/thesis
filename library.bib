journal = {The Journal of neuroscience : the official journal of the Society for Neuroscience},
keywords = {birdsong,central pattern generator,proportional scaling,rhythm,sequence production,temporal structure},
number = {3},
pages = {991--1005},
pmid = {16421319},
title = {{Temporal structure in zebra finch song: implications for motor coding.}},
volume = {26},
year = {2006}
}
