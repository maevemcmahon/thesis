@article{Pavlov2011a,
abstract = {Traumatic brain injury (TBI) is a risk factor for the development of epilepsy, which can occur months to years after the insult. The hippocampus is particularly vulnerable to the pathophysiological effects of TBI. Here, we determined whether there are long-term changes in inhibition in the dentate gyrus that could contribute to the progressive susceptibility to seizures after TBI. We used severe lateral-fluid percussion brain injury to induce TBI in rats. In this model, spontaneous seizure activity, which involves the hippocampus, appears after a long latent period, resembling the human condition. We demonstrate that synaptic GABA(A) receptor-mediated inhibition is profoundly reduced in ipsilateral dentate granule cells 1 month after TBI. Moreover, synaptic inhibition decreases over time, and by 6 months after TBI, it is also significantly decreased contralaterally. Progressive loss of synaptic inhibition is paralleled by a decline in the number of parvalbumin-positive interneurons, but, in contrast to status epilepticus models, GABA(A) receptor subunit expression is largely unaltered. At both time points, the magnitude of tonic GABA(A) receptor-mediated currents after TBI is maintained, indicating a preservation of the inhibitory constraint of granule cells through tonic inhibition. Our results extend the time window during which strategies to target epileptogenesis may be effective.},
author = {Pavlov, I and Huusko, N and Drexel, M and Kirchmair, E and Sperk, G{\"{u}}nther and Pitk{\"{a}}nen, A and Walker, Matthew C},
doi = {10.1016/j.neuroscience.2011.07.074},
file = {:home/maeve/Documents/Mendeley Desktop/Pavlov et al. - 2011 - Progressive loss of phasic, but not tonic, GABA(A) receptor-mediated inhibition in dentate granule cells in a mod.pdf:pdf},
issn = {1873-7544},
journal = {Neuroscience},
keywords = {20,2006,Animal,Animals,Brain Injuries,Brain Injuries: complications,Brain Injuries: physiopathology,Dentate Gyrus,Dentate Gyrus: pathology,Dentate Gyrus: physiopathology,Disease Models,Disease Progression,Epilepsy,GABA-A,GABA-A: drug effects,GABA-A: physiology,Interneurons,Interneurons: drug effects,Interneurons: pathology,Interneurons: physiology,Male,Neural Inhibition,Neural Inhibition: drug effects,Neural Inhibition: physiology,Rats,Receptors,Sprague-Dawley,Temporal Lobe,Temporal Lobe: etiology,Temporal Lobe: pathology,Temporal Lobe: physiopathology,accounts for up to,agrawal et al,dentate gyrus,gaba a receptors,interneurons,of,parvalbumin,seizures,symptomatic epilepsies,tbi,tonic inhibition,traumatic brain injury},
