journal = {J Neurophysiol},
keywords = {*Brain Mapping,*Noise,Acoustic Stimulation/methods,Animals,Auditory Perception/*physiology,Behavior, Animal,Discrimination (Psychology)/*physiology,Finches/anatomy {\&} histology/*physiology,Image Processing, Computer-Assisted,Magnetic Resonance Imaging,Male,Oxygen/blood,Prosencephalon/anatomy {\&} histology/*blood supply/p,Vocalization, Animal/*physiology},
number = {2},
