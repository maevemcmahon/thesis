journal = {Cerebellum (London, England)},
keywords = {Afferent Pathways,Afferent Pathways: physiology,Animals,Cerebellar Nuclei,Cerebellar Nuclei: physiology,Cerebellum,Cerebellum: physiology,Conditioning, Classical,Conditioning, Classical: physiology,Decerebrate State,Electric Stimulation,Extinction, Psychological,Extinction, Psychological: physiology,Ferrets,Forelimb,Forelimb: innervation,Learning,Nerve Fibers,Nerve Fibers: physiology,Neurons,Neurons: physiology,Olivary Nucleus,Olivary Nucleus: physiology,Photic Stimulation,Purkinje Cells,Purkinje Cells: physiology,Reaction Time},
