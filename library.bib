journal = {Human Brain Mapping},
keywords = {brain,caudate nucleus,globus,magnetic resonance imaging,nucleus accumbens,pallidus,putamen,sex differences,volumetry},
month = {apr},
pmid = {21523857},
title = {{Normal sexual dimorphism in the human basal ganglia}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21523857},
volume = {Early Onli},
year = {2011}
}
