isbn = {1545-7885 (Electronic)$\backslash$r1544-9173 (Linking)},
issn = {1545-7885},
journal = {PLoS biology},
keywords = {Acoustic Stimulation,Animal,Animal: physiology,Animals,Avian Proteins,Avian Proteins: genetics,Behavior,Cell Count,Female,Finches,Finches: physiology,Gene Expression Regulation,Male,Mating Preference,Social Behavior,Telencephalon,Telencephalon: cytology,Telencephalon: metabolism,Vocalization},
