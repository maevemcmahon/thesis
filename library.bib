file = {:home/maeve/Documents/Mendeley Desktop/Lehongre et al. - 2011 - Altered low-$\gamma$ sampling in auditory cortex accounts for the three main facets of dyslexia.pdf:pdf},
issn = {1097-4199},
journal = {Neuron},
