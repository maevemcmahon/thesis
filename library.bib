pages = {4--17},
publisher = {Elsevier Srl},
title = {{On the role and origin of isochrony in human rhythmic entrainment}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19046745},
volume = {45},
year = {2009}
}
