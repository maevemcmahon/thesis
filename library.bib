issn = {1090-2155},
journal = {Brain {\&} Language},
keywords = {Animals,Biological Evolution,Brain,Brain: physiology,Gestures,Humans,Language,Motor Activity,Motor Activity: physiology,Neurons,Neurons: physiology,Perception,Perception: physiology},
month = {jan},
