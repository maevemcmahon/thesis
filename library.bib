month = {oct},
number = {4},
pages = {1771--81},
pmid = {16777432},
title = {{Interactions between auditory and dorsal premotor cortex during synchronization to musical rhythms.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16777432},
volume = {32},
year = {2006}
}
