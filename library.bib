keywords = {Animals,Cerebellar Cortex,Cerebellar Cortex: anatomy {\&} histology,Cerebellar Cortex: metabolism,Female,Male,Mole Rats,Mole Rats: anatomy {\&} histology,Nerve Tissue Proteins,Nerve Tissue Proteins: metabolism,Phospholipase C beta,Phospholipase C beta: metabolism,Purkinje Cells,Purkinje Cells: metabolism},
month = {oct},
