month = {jan},
number = {1},
pages = {e14620},
pmid = {21297968},
title = {{Behavioural dissociation between exogenous and endogenous temporal orienting of attention.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3030556{\&}tool=pmcentrez{\&}rendertype=abstract},
