issn = {0962-8924},
journal = {Trends in Cell Biology},
keywords = {Animals,Cell Differentiation,Cell Differentiation: physiology,Drosophila,Drosophila: cytology,Drosophila: physiology,Eye,Eye: cytology,Humans,Retina,Retina: cytology,Retina: physiology},
