pages = {251--5},
pmid = {22699618},
publisher = {Nature Publishing Group},
title = {{Covert skill learning in a cortical-basal ganglia circuit.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3377745{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {486},
year = {2012}
}
