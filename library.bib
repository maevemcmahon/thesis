isbn = {1200655109},
issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
month = {oct},
pages = {17245--52},
pmid = {23045667},
title = {{Impact of experience-dependent and -independent factors on gene expression in songbird brain.}},
