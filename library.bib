journal = {The Journal of comparative neurology},
keywords = {Animals,Cerebellum,Cerebellum: cytology,Fructose-Bisphosphate Aldolase,Fructose-Bisphosphate Aldolase: metabolism,Models, Anatomic,Neural Pathways,Neural Pathways: cytology,Purkinje Cells,Purkinje Cells: cytology,Purkinje Cells: enzymology,Rats,Rats, Long-Evans},
month = {jan},
number = {2},
pages = {282--304},
pmid = {19003905},
title = {{Projection of reconstructed single Purkinje cell axons in relation to the cortical and nuclear aldolase C compartments of the rat cerebellum.}},
