keywords = {Attention,Attention: physiology,Awareness,Awareness: physiology,Brain Mapping,Dementia,Dementia: diagnosis,Dementia: physiopathology,Humans,Mental Recall,Mental Recall: physiology,Neuropsychological Tests,Problem Solving,Problem Solving: physiology},
