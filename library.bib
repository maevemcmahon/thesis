keywords = {Aptitude,Aptitude: physiology,Autistic Disorder,Autistic Disorder: physiopathology,Autistic Disorder: psychology,Creativeness,Humans,Memory,Memory: physiology,Models,Pattern Recognition,Perception,Perception: physiology,Physiological,Physiological: physiology,Psychological},
month = {may},
number = {1522},
pages = {1385--91},
title = {{Enhanced perception in savant syndrome: patterns, structure and creativity.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2677591{\&}tool=pmcentrez{\&}rendertype=abstract http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2677591{\%}7B{\&}{\%}7Dtool=pmcentrez{\%}7B{\&}{\%}7Drendertype=abstract},
